package coding100days;

public class Day3 {

    public static void main(String[] args) {

        //komentar satu baris (Single-line Comment)
        System.out.println("hello dunia");

        /* Ini adalah komentar banyak baris
           Biasanya dipakai untuk menjelaskan
           bagian kode yang agak panjang ( Multi-line Comment)*/
        System.out.println("coding 100 hari");
    }

}
